{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "listings/listing-info",
	"version": "0.1.0",
	"title": "Listing Info",
	"category": "theme",
	"icon": "home",
	"description": "View information about specific Listing.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"listingId": {
			"type": "string",
			"default": ""
		},
		"showRooms": {
			"type": "boolean",
			"default": false
		},
		"showBedrooms": {
			"type": "boolean",
			"default": false
		},
		"showBathrooms": {
			"type": "boolean",
			"default": false
		},
		"showGarageCapacity": {
			"type": "boolean",
			"default": false
		},
		"showArea": {
			"type": "boolean",
			"default": false
		},
		"showYearBuilt": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "listings",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}